<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="patent" >
  	
  <typeAlias type="com.qianyan.search.db.model.VwSearchPatent" alias="Patent" />
  	
  <resultMap id="patentBean" class="Patent" >
    <result column="region_id" property="c1"/>
    <result column="name" property="c2"/>
    <result column="fullname" property="c3"/>
    <result column="parent_id" property="c4"/>
    <result column="region_code" property="c5"/>
    <result column="shop_num" property="c6"/>
    <result column="max_shop_num" property="c7"/>
    <result column="category_number" property="c8"/>
  </resultMap>
  
  <select id="getSupplyList" parameterClass="int" resultMap="patentBean">
	select top 500 * from region_tree where parent_id=#parentId#
  </select>
	 	
  
  
</sqlMap>